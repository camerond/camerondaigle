{% for s in story %}
  <section class="story" id={{s.slug}}>
    <div class="story__logo">
      <img src="/img/logos/{{s.slug}}.svg" />
    </div>
    <div class="story__main">
      <div class="story__text">
        <h2>{{s.title}}</h2>
        <h3>{{s.company or "Reverb"}}</h3>
        {% for c in s.content %}
          <p>{{ c }}</p>
        {% endfor %}
      </div>
      <div
        class="story__image"
        style="background-image: url(/img/story/{{s.slug}}.jpg)"
      />
    </div>
  </section>
{% endfor %}

{% endblock %}